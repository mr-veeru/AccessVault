# Flask secret keys
SECRET_KEY=
JWT_SECRET_KEY=

# PostgreSQL database URL
SQLALCHEMY_DATABASE_URI=

# Redis Configuration (Optional - defaults to memory://)
# Blocklist Redis URL (Optional - uses RATELIMIT_STORAGE_URL if not set)
RATELIMIT_STORAGE_URL=redis://localhost:6379/0
BLOCKLIST_REDIS_URL=redis://localhost:6379/1

# Flask Environment (development, production, testing)
FLASK_ENV=development

# Flask Debug Mode (true/false)
FLASK_DEBUG=false

# CORS Configuration (Optional - defaults to * for development)
# Comma-separated list of allowed origins (e.g., http://localhost:3000,https://example.com)
# Use * for development (allows all origins) - NOT recommended for production
CORS_ORIGINS=*

# Log Level
LOG_LEVEL=INFO